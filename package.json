{
  "name": "spacetime",
  "version": "2.2.0",
  "description": "represent dates in remote timezones",
  "main": "./spacetime.js",
  "module": "./es.js",
  "license": "Apache-2.0",
  "scripts": {
    "esm": "rollup -c rollup.config.es.js && uglifyjs -o es.js --compress -- esm.js && rm esm.js",
    "cjs": "rollup -c rollup.config.cjs.js && uglifyjs -o spacetime.js --compress -- spacetime.raw.js && rm spacetime.raw.js",
    "precommit": "lint-staged",
    "build": "npm run cjs && npm run esm",
    "demo": "node ./scripts/demo.js",
    "watch": "node ./scripts/watch.js",
    "test": "tape ./test/**/*.test.js | tap-dot",
    "lint": "eslint .",
    "size": "./node_modules/.bin/size-limit",
    "prepublishOnly": "./node_modules/.bin/size-limit",
    "coverage": "node ./scripts/coverage.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smallwins/spacetime.git"
  },
  "files": [
    "es.js",
    "spacetime.js"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^3.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "gaze": "^1.1.1",
    "nyc": "^8.4.0",
    "prettier": "^1.5.3",
    "rollup": "^0.52.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "shelljs": "^0.7.2",
    "size-limit": "^0.8.0",
    "tap-dot": "^1.0.5",
    "tape": "4.6.0",
    "timekeeper": "^1.0.0",
    "uglify-es": "^3.2.2"
  },
  "size-limit": [
    {
      "path": "./spacetime.js",
      "limit": "14 KB"
    }
  ]
}
